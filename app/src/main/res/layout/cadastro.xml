<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">
    <TextView
      android:id="@+id/cadastrar"
      android:layout_height="24dp"
       android:layout_width="83dp"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintStart_toStartOf="parent"
      android:layout_marginTop="619dp"
      android:layout_marginStart="141dp"
      android:text="Cadastrar"
      android:textSize="20sp"
      android:textColor="#ffffff" />

    <TextView
        android:id="@+id/bg"
        android:layout_width="388dp"
        android:layout_height="778dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="4dp"
        android:textColor="#000000"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnCadastro"
        android:layout_width="283dp"
        android:layout_height="44dp"
        android:layout_marginTop="640dp"
        android:backgroundTint="#0843f6"
        android:onClick="validar_dados_cadastro"
        android:padding="10dp"
        android:text="Cadastrar"
        android:textColor="#ffffff"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imagecadastro"
        android:layout_width="226dp"
        android:layout_height="226dp"
        android:layout_marginTop="48dp"
        android:scaleType="centerCrop"
        android:src="@drawable/imagecadastro"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/nome"
        android:layout_width="84dp"
        android:layout_height="27dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="244dp"
        android:text="Nome:"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editNomeCadastro"
        android:layout_width="296dp"

        android:layout_height="41dp"
        android:layout_marginTop="284dp"
        android:background="#939393"
        android:hint="Nome"
        android:padding="10dp"
        android:paddingStart="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/email"
        android:layout_width="84dp"
        android:layout_height="27dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="348dp"
        android:text="Email:"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editEmailCadastro"
        android:layout_width="296dp"

        android:layout_height="41dp"
        android:layout_marginTop="380dp"
        android:background="#939393"
        android:hint="email"
        android:padding="10dp"
        android:paddingStart="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editTelefoneCadastro"
        android:layout_width="296dp"

        android:layout_height="41dp"
        android:layout_marginTop="468dp"
        android:background="#939393"
        android:hint="Telefone"
        android:padding="10dp"
        android:paddingStart="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/telefone2"
        android:layout_width="84dp"
        android:layout_height="27dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="520dp"
        android:text="Senha:"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/editSenhaCadastro"
        android:layout_width="294dp"
        android:layout_height="41dp"
        android:layout_marginTop="556dp"
        android:background="#939393"
        android:hint="Senha"
        android:padding="10dp"
        android:paddingStart="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/telefone"
        android:layout_width="84dp"
        android:layout_height="27dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="436dp"
        android:text="Telefone:"
        android:textColor="#000000"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>